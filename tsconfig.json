{
    "compilerOptions": {
        "baseUrl": ".",
        "strictNullChecks": true,
        "moduleResolution": "node",
        "module": "esnext",
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "jsx": "preserve",
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "target": "es6",
        "paths": {
            "dva/dynamic": [
                "src/types/dva/dynamic"
            ]
        },
        "lib": [
            "dom",
            "es7"
        ]
    },
  "exclude": [
      "node_modules",
      "lib",
      "es"
  ]
}
